def find_min_index(lst):
    if not lst:
        return None
    min_val = lst[0]
    min_idx = 0
    for i in range(1, len(lst)):
        if lst[i] < min_val:
            min_val = lst[i]
            min_idx = i
    return min_idx


def sort_numbers(s):
    nums = s.split()
    nums = [int(x) for x in nums]
    nums.sort()
    return ' '.join(map(str, nums))
print(sort_numbers('2 1 3 4 7 6 5 9 8'))
